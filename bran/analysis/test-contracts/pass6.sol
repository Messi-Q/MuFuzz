pragma solidity ^0.4.0;

contract Pass6 {
    function add(int a) public pure returns (int) {
        int b = 0;
        if (b == 0) {
            b = 20 + 2;
            b = b - 1;
            b = b * 2;
            b = b / 2; // b == 21
        }
        assert(b == 21);
        if ((b == 100) || (b == 21)) {
            b = b * 2;
        }
        assert(b == 42);
        if ((b == 42) && (b == 30)) {
            b = b / 2;
        }
        if (a > 100) {
            // does not affect b
        }
        assert(b == 42);
        if (!(b == 42)) {
            assert(false);
            b = 10;
        }
        if (b % 2 == 0) {
            b = b;
        } else {
            assert(false);
            b = 0;
        }
        assert(b == 42);
        if ((b ^ 42) != 0) {
            assert(false);
        }
        if (b != (1 << 5) + 10) {
            assert(false);
        }
        if (b != (84 >> 1)) {
            assert(false);
        }
        if (2**3 != 2 << 2) {
            assert(false);
        }
        assert(b == 100 || (b == 42 && b != 200));
    }
}

